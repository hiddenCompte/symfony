<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="466.0" prefWidth="750.0" stylesheets="@CssModifierReclamation.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="controllers.ModifierReclamationController">
    <Label layoutX="14.0" layoutY="31.0" styleClass="label" text="Titre de la réclamation:" />
    <TextField fx:id="titreTextField" layoutX="352.0" layoutY="18.0" promptText="Entrez le titre" styleClass="text-field" />

    <Label layoutY="122.0" styleClass="label" text="Description de la réclamation:" />
    <TextField fx:id="descriptionTextField" layoutX="314.0" layoutY="102.0" prefHeight="194.0" prefWidth="323.0" promptText="Entrez la description" styleClass="text-field" />

    <Button layoutX="430.0" layoutY="349.0" onAction="#modifierReclamation" styleClass="button" text="Modifier" />
    <Button layoutX="620.0" layoutY="349.0" onAction="#annulerReclamation" styleClass="button cancel" text="Annuler" />
</AnchorPane>
